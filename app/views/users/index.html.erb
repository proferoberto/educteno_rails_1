<h1>Users#index</h1>
<h2>Hola clase edutecno</h2>
<div>
	<%= link_to 'Agregar usuario', new_user_path %>	
</div>

<table>
	<thead>
		<tr>Name</tr>
		<tr>Lastname</tr>
		<tr>Email</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name%></td>
				<td><%= user.lastname%></td>
				<td><%= user.email%></td>
			</tr>
		<% end %>
	</tbody>
</table>

